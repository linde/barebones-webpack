{
  "name": "barebones-webpack",
  "version": "0.0.9",
  "description": "",
  "main": "index.js",
  "config": {
    "mode": "none",
    "dev_port": "8888",
    "e2e_port": "8889",
    "dir": "dist"
  },
  "scripts": {
    "compile": "webpack --mode=${npm_package_config_mode}",
    "dev-webpack-server": "webpack-dev-server --mode=${npm_package_config_mode}",
    "jest-unit": "jest --testPathIgnorePatterns=.*e2e.test.js ${WATCH_ARG}",
    "jest-e2e": "jest --testPathPattern=.*e2e.test.js$ ${WATCH_ARG}",
    "test": "jest ${WATCH_ARG}",
    "build": "npm run compile && npm run test",
    "dev": "WATCH_ARG=--watch concurrently npm:dev-webpack-server npm:test"
  },
  "jest": {
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {},
    "preset": "jest-puppeteer"
  },
  "author": "",
  "license": "NONE",
  "devDependencies": {
    "@popperjs/core": "^2.11.5",
    "bootstrap": "^5.1.3",
    "concurrently": "^7.3.0",
    "css-loader": "^6.7.1",
    "d3": "^7.6.1",
    "dc": "^4.2.7",
    "html-webpack-plugin": "^5.5.0",
    "http-server": "^14.1.1",
    "jest": "^28.1.3",
    "jest-puppeteer": "^6.1.1",
    "jquery": "^3.6.0",
    "puppeteer": "^16.1.0",
    "style-loader": "^3.3.1",
    "wait-on": "^6.0.1",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  }
}
